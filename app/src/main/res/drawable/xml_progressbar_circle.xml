<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item n1:id="@android:id/background"
        xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape
            n1:shape="ring"
            n1:useLevel="false"
            n1:innerRadius="64.0dip"
            n1:thickness="3.0dip">
            <solid n1:color="#ffffffff" />
        </shape>
    </item>
    <item n2:id="@android:id/progress"
        xmlns:n2="http://schemas.android.com/apk/res/android">
        <clip>
            <shape
                n2:shape="ring"
                n2:useLevel="false"
                n2:innerRadius="64.0dip"
                n2:thickness="3.0dip">
                <solid n2:color="#ffff5252" />
            </shape>
        </clip>
    </item>
</layer-list>