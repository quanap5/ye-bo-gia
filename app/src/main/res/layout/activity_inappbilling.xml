<?xml version="1.0" encoding="utf-8"?>
<FrameLayout n1:layout_gravity="center"
    n1:id="@id/rootFrameLayout"
    n1:background="@android:color/transparent"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent"
    xmlns:n1="http://schemas.android.com/apk/res/android">

    <com.yeoboya.com.yeoboya.WebView.xwalkview.BaseXwalkView
        n1:gravity="center"
        n1:layout_gravity="center"
        n1:id="@id/inappView_WebView"
        n1:layout_width="fill_parent"
        n1:layout_height="fill_parent" />

    <LinearLayout
        n1:gravity="center"
        n1:layout_gravity="center"
        n1:orientation="vertical"
        n1:id="@id/screen_wait1"
        n1:visibility="gone"
        n1:layout_width="fill_parent"
        n1:layout_height="fill_parent">

        <TextView
            n1:textSize="20.0dip"
            n1:textColor="#ff393c51"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:text="@string/text_waiting" />
    </LinearLayout>
</FrameLayout>