<?xml version="1.0" encoding="utf-8"?>
<LinearLayout n1:orientation="vertical"
    n1:id="@id/activity_main"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent"
    xmlns:n1="http://schemas.android.com/apk/res/android">

    <RelativeLayout
        n1:background="@drawable/sns_title_bg"
        n1:layout_width="fill_parent"
        n1:layout_height="wrap_content">

        <ImageView
            n1:id="@id/sns_login_back"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:src="@drawable/back_btn"
            n1:layout_alignParentLeft="true" />

        <TextView
            n1:textSize="18.0sp"
            n1:textStyle="bold"
            n1:textColor="#ffffffff"
            n1:id="@id/sns_login_title"
            n1:padding="2.0dip"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:text="로그인"
            n1:textScaleX="0.9"
            n1:layout_centerInParent="true" />
    </RelativeLayout>

    <com.yeoboya.com.yeoboya.WebView.xwalkview.BaseXwalkView
        n1:id="@id/sns_login_Webview"
        n1:layout_width="fill_parent"
        n1:layout_height="fill_parent"
        n1:layout_alignParentLeft="true"
        n1:layout_alignParentTop="true" />
</LinearLayout>